---
import Nav from './ComposableNav.astro';

const headerItems = [
  { name: 'Downloads', href: '/download' },
  { name: 'SDKs', href: '/docs/v1/tech/client-libraries' },
  { name: 'API Docs', href: '/docs/v1/tech/apis' },
  { name: 'Technical Articles', href: '/docs/v1/tech', foldsIntoMenu: true },
  { name: 'Blog', href: '/blog/', foldsIntoMenu: true },
  { name: 'Developer Tools', href: '/dev-tools/', foldsIntoMenu: true },
  { name: 'Release Notes', href: '/docs/v1/tech/release-notes', foldsIntoMenu: true },
]

const buttons = [ 'divider', 'dark-mode', 'search-small', 'expert' ];

const mobileAfterBreakItems = [
  { name: 'Login', href: 'https://account.fusionauth.io/account/' },
  { name: 'Talk To An Expert', href: '/contact'},
]
---
<Nav sectionTitle="Docs"
     sectionTitleHref="/docs/"
     headerItems={ headerItems }
     buttons={ buttons }
     mobileAfterBreakItems={ mobileAfterBreakItems } />
