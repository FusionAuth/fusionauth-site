---
import Nav from './ComposableNav.astro';

const headerItems = [
  { name: 'Tutorial', href: '/docs/quickstarts/'},
  { name: 'Features', href: '/docs/quickstarts/'},
  { name: 'Comparison', href: '/docs/' },
  { name: 'Community', href: '/docs/', foldsIntoMenu: true },
  { name: 'Article', href: '/docs/', foldsIntoMenu: true },
  { name: 'Announcement', href: '/docs/', foldsIntoMenu: true },
  { name: 'Tech Docs', href: '/download', hideOnMobile: true},
];

const buttons = ['expert', 'search'];

const mobileAfterBreakItems = [
  { name: 'Talk To An Expert', href: '/contact'}
]
---
<Nav sectionTitle="Blog"
     sectionTitleHref="/blog/"
     headerItems={ headerItems }
     buttons={ buttons }
     mobileAfterBreakItems={ mobileAfterBreakItems }/>