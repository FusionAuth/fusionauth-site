---
import Nav from './ComposableNav.astro';

const headerItems = [
  {name: 'Tutorial', href: '/blog/category/tutorial/'},
  {name: 'Product Updates', href: '/blog/category/product/'},
  {name: 'Comparison', href: '/blog/category/compare/'},
  {name: 'Community', href: '/blog/category/community/', foldsIntoMenu: true},
  {name: 'Education', href: '/blog/category/education/', foldsIntoMenu: true},
  {name: 'News', href: '/blog/category/news/', foldsIntoMenu: true},
  {name: 'Tech Docs', href: '/docs/', hideOnMobile: true},
];

const buttons = ['expert', 'search'];

const mobileAfterBreakItems = [
  { name: 'Talk To An Expert', href: '/contact'}
]
---
<Nav sectionTitle="Blog"
     sectionTitleHref="/blog/"
     headerItems={ headerItems }
     buttons={ buttons }
     mobileAfterBreakItems={ mobileAfterBreakItems }
     colorThemeOverride="dark" />
