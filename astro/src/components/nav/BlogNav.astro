---
import Nav from './ComposableNav.astro';

const headerItems = [
  { name: 'Tutorial', href: '/blog/category/tutorial/'},
  { name: 'Product Updates', href: '/blog/category/features/'},
  { name: 'Comparison', href: '/blog/category/comparison/' },
  { name: 'Community', href: '/blog/category/community-story/', foldsIntoMenu: true },
  { name: 'Article', href: '/blog/category/article/', foldsIntoMenu: true },
  { name: 'News', href: '/blog/category/announcement//', foldsIntoMenu: true },
  { name: 'Tech Docs', href: '/docs/', hideOnMobile: true},
];

const buttons = ['expert', 'search'];

const mobileAfterBreakItems = [
  { name: 'Talk To An Expert', href: '/contact'}
]
---
<Nav sectionTitle="Blog"
     sectionTitleHref="/blog/"
     headerItems={ headerItems }
     buttons={ buttons }
     mobileAfterBreakItems={ mobileAfterBreakItems }/>
