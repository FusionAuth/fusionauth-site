---
import DarkModeButton from './DarkModeButton.astro';
import Divider from './Divider.astro';
import ExpertNavButton from './ExpertNavButton.astro';
import LoginButton from './LoginButton.astro';
import SearchButton from './SearchButton.astro';

const { name } = Astro.props;
---
{
  {
    'dark-mode': <DarkModeButton/>,
    'divider': <Divider/>,
    'expert': <ExpertNavButton/>,
    'login': <LoginButton/>,
    'search': <SearchButton/>,
    'search-small': <SearchButton size="small"/>
  }[name]
}