---
import Diagram from "../../../components/mermaid/SequenceDiagram.astro";
const { alt } = Astro.props;

//language=Mermaid
const diagram = `
sequenceDiagram

  actor user as User/Browser
  participant oldauth as Old Auth System
  participant oldauthds as Old Auth Datastore
  user ->> oldauth : Provide credentials
  oldauth ->> oldauthds : Verify credentials
  oldauthds ->> oldauth : User data
  oldauth ->> user : Authentication complete
`;
---
<Diagram code={diagram} alt={alt}/>
