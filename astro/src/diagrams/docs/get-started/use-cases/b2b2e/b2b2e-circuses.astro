---
import Diagram from 'src/components/mermaid/FlowchartDiagram.astro';

const { alt } = Astro.props;

//language=Mermaid
const code = `
graph TD
    clowntrax[ClownTrax] --> |Sells time tracking system to| ringling[Ringling Brothers]
    clowntrax --> |Sells time tracking system to| cirque[Cirque De Soleil]
    ringling --> |Requires login from| employeeA[Alex Taylor]
    ringling --> |Sells tickets to| employeeB[Jamie Morgan]
    cirque --> |Sells tickets to| employeeB
    cirque --> |Sells tickets to| employeeC[Chris Jordan]
    cirque --> |Sells tickets to| employeeD[AcmeCorp]
`;

---
<Diagram {code} alt={alt}/>

