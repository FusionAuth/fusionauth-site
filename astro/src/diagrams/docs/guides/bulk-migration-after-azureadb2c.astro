---
import Diagram from "../../../components/mermaid/SequenceDiagram.astro";
const { alt } = Astro.props;

//language=Mermaid
const diagram = `
sequenceDiagram

actor user as User Agent
participant sys as New System

user ->> sys  : Request login page
sys -->> user  : Receive login page
user ->> sys  : Post credentials
sys ->> sys : Verify credentials
sys -->> user  : Display results
`;
---
<Diagram code={diagram} alt={alt}/>

