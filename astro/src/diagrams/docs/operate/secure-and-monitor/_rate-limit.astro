---
import Diagram from 'src/components/mermaid/FlowchartDiagram.astro';

//language=Mermaid
const code = `
flowchart LR
    A(User attempts to log in) --> B{{Has user exceeded rate limit?}}
    B -- No --> C(User may log in)
    B -- Yes --> D(User may not log in)
`;
---
<Diagram {code} />
<style is:global>
  foreignObject > div {
    transform: translateY(-4px);
  }
</style>

