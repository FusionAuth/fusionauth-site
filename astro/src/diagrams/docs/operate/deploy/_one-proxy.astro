---
import Diagram from 'src/components/mermaid/SequenceDiagram.astro';

const { alt } = Astro.props;

//language=Mermaid
const diagram = `
sequenceDiagram

participant u as User Agent
participant p as Proxy
participant f as FusionAuth

u ->> p : Request
p ->> f : Request 
f -->> p : Send page
p -->> u : Send page
`;
---
<Diagram code={diagram} alt={alt}/>

