---
import Diagram from 'src/components/mermaid/SequenceDiagram.astro';

const { alt } = Astro.props;

//language=Mermaid
const diagram = `
sequenceDiagram

participant u as User Agent
participant p1 as Proxy 1
participant p2 as Proxy 2
participant f as FusionAuth

u ->> p1  : Request
p1 ->> p2 : Request 
p2 ->> f  : Request 
f -->> p2  : Send page
p2 -->> p1 : Send page
p1 -->> u  : Send page
`;
---
<Diagram code={diagram} alt={alt}/>

