---
import Diagram from 'src/components/mermaid/SequenceDiagram.astro';

const { alt } = Astro.props;

//language=Mermaid
const code = `
sequenceDiagram
    participant Browser
    participant cb as ChangeBank
    participant FusionAuth
    Browser ->> cb : Request Home Page
    cb ->> cb : Check Application Session Valid
    cb ->> Browser : Sends Home Page
    Browser ->> cb : Request Calculate Change Page
    cb ->> cb : Check Application Session Valid
    cb ->> Browser : Sends Calculate Change Page
`;

---
<Diagram {code} {alt} />
