---
import Layout from "../../../layouts/BlogIndex.astro";
import { getLatestStaticPaths } from "../../blog/blog-tools";

export async function getStaticPaths({ paginate }) {
  return getLatestStaticPaths(paginate, "podcasts");
}

const { page } = Astro.props;
---

<Layout
  title="Latest podcasts"
  section="latest"
  sectionTitle="Podcasts"
  subjectName="Latest Podcasts"
  page={page}
  showCategories={false}
  type="podcasts"
/>
