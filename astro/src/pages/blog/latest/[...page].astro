---
import Layout from '../../../layouts/BlogIndex.astro';
import { getLatestStaticPaths } from 'src/tools/blog';

export async function getStaticPaths({ paginate }) {
  return getLatestStaticPaths(paginate);
}

const { page } = Astro.props;
---
<Layout title="Latest blog posts"
        description="FusionAuth Blog | Stay updated with the latest posts on authentication trends and news."
        section="latest"
        sectionTitle="Blogs"
        subjectName="Latest Posts"
        page={page}
        showCategories={false}/>
