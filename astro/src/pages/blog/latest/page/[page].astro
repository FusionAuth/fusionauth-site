---
import Layout from '../../../../layouts/BlogIndex.astro';
import { getLatestStaticPaths } from '../../blog-tools';

export async function getStaticPaths({ paginate }) {
  return getLatestStaticPaths(paginate);
}

const { page } = Astro.props;
---
<Layout section="latest"
        sectionTitle="Blogs"
        subject="Latest Posts"
        page={page}
        showCategories={false}/>