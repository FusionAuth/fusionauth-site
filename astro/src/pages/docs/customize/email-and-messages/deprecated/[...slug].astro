---
import Layout from 'src/layouts/Docs.astro';
import { getStaticDocPaths, getDocNavContext } from 'src/tools/docs';

export const getStaticPaths = getStaticDocPaths('customize/email-and-messages/deprecated/');

const navContext = await getDocNavContext(Astro);
const { entry } = Astro.props;
const { Content, headings } = await entry.render();
---
<Layout frontmatter={entry.data} {headings} {navContext}>
  <Content />
</Layout>
