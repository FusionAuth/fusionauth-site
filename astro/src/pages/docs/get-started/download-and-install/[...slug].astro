---
import Layout from "../../../../layouts/Docs.astro";
import { getStaticDocPaths } from '../../doc-tools';

export const getStaticPaths = getStaticDocPaths('get-started/download-and-install/');

const { entry } = Astro.props;
const { Content, headings } = await entry.render();
---
<Layout frontmatter={entry.data} {headings}>
  <Content />
</Layout>
