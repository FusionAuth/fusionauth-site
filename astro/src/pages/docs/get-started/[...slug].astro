---
import Layout from 'src/layouts/Docs.astro';
import { getStaticDocPaths } from 'src/pages/docs/doc-tools';

export const getStaticPaths = getStaticDocPaths('get-started/');

const { entry } = Astro.props;
const { Content, headings } = await entry.render();
---
<Layout frontmatter={entry.data} {headings}>
  <Content />
</Layout>
