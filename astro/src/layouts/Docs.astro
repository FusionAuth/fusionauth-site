---
import Default from './Default.astro';
import DocsNav from '../components/nav/DocsNav.astro';

let { frontmatter, headings, navContext } = Astro.props;
const { sideMenu, sections } = navContext ? navContext : {};
---
<Default {frontmatter} {headings} cta="NONE" contribute={false} {sideMenu} {sections}  {...Astro.props}>
  <DocsNav slot="nav" />
  <slot></slot>
</Default>
