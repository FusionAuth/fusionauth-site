---
import { getCollection } from "astro:content";
import Default from "./Default.astro";


const openGraphImage = '/img/og/quickstarts-developers-v1.png';

import { quickstartSections } from '../pages/docs/quickstarts/quickstart-sections';
import DocsNav from '../components/nav/DocsNav.astro';

const sections = quickstartSections
  .map(qsSection=> ({
    title: qsSection.title,
    path: '/docs/quickstarts#'+qsSection.anchorTag,
    icon: qsSection.faIcon,
  }))
---
<Default sections={sections} sideMenu={[]} {openGraphImage} {...Astro.props}>
  <DocsNav slot="nav" darkModeToggle={true}/>
  <slot/>
</Default>
