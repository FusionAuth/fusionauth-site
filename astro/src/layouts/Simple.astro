---
import Default from "./Default.astro";
import DocsNav from "src/components/nav/DocsNav.astro";
let { frontmatter, headings, navContext, noSideNav } = Astro.props;
---

<Default
  {frontmatter}
  {headings}
  cta="NONE"
  copyMarkdown="true"
  contribute={false}
  maxTocDepth={3}
  {...Astro.props}
>
  <DocsNav {noSideNav} slot="nav" darkModeToggle={true} />
  <slot />
</Default>
